<DialogPane minHeight="280.0" minWidth="539.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Dashboard.Store.StoreController">
   <content>
      <AnchorPane prefHeight="280.0" prefWidth="539.0">
         <children>
            <AnchorPane id="achPaneTopBar" prefHeight="26.0" prefWidth="539.0" stylesheets="@../../Styles/achStyle.css">
